                </Power>
                <Power>
                    <Value>SortingManager</Value>
                    <Text>分拣主管</Text>
                </Power>
                <Power>
                    <Value>StationManager</Value>
                    <Text>站长</Text>
                </Power>
                <Power>
                    <Value>CustomerSupportManager</Value>
                    <Text>客服主管</Text>
                </Power>
                <Power>
                    <Value>CustomerSupport</Value>
                    <Text>客服</Text>
                </Power>
                <Power>
                    <Value>Supplier</Value>
                    <Text>客户</Text>
                </Power>
            </Powers>
        </x:XData>
